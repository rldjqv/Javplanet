<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="kr.co.javplanet.api.upload.dao.UploadDao">

	<sql id="conditions">
		<!-- <if test="data.app_seq != null and data.app_seq != ''">
			AND master.tn_appinfo.app_seq = #{data.data.app_seq}			
		</if> -->		
	</sql>
	
	<insert id="postUpload" parameterType="kr.co.javplanet.api.upload.model.UploadParam" useGeneratedKeys="true" keyProperty="data.seq">
		INSERT INTO /* upload.xml : postUpload */
			jav_board
			(
			title,
			contents,
			regDate
			)
		VALUES
			(
			#{data.title},
			#{data.contents},
			now()
			)
		<include refid="conditions"/>
	</insert>
	
</mapper>